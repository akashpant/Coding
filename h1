#include <iostream>

using namespace std;
    int main()
    {
        long n,a[100000],b[100000];
        int i;
        cin>>n;
        for(i=0;i<n;i++)
        {
            cin>>a[i];
        }
        for(i=0;i<n;i++)
        {
            cin>>b[i];
            cout<<a[i]+b[i];
            cout<<" ";

        }

        return 0;
    }
